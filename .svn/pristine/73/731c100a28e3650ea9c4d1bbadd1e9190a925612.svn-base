@charset "UTF-8";
*{
	margin: 0;
	padding: 0;
}
#contents_outline{
	width: 1000px;
	height: 500px;
	margin: 50px auto;
	box-sizing: border-box;
	/* border: 1px solid black; */
}
#generalBoard_area{
	width: 300px;
	height: 400px;
	margin: 50px auto;
	margin-left: 100px;
	float: left;
	box-sizing: border-box;
	border: 1px solid red;
}
#ajaxBoard_area{
	width: 300px;
	height: 400px;
	margin: 50px auto;
	margin-right: 100px;
	float: right;
	box-sizing: border-box;
	border: 1px solid blue;
}
.title_area{
	width: 200px;
	margin: 0 auto;
	margin-bottom: 60px;
	margin-top: 30px;
	box-sizing: border-box;
	/* border: 1px solid black; */
}
.title_area h1{
	font-size: 50px;
	text-align: center;
}
.category{
	width: 200px;
	height: 39px;
	margin: 0 auto;
	margin-top: 50px;
	box-sizing: border-box;
	font-size: 22px;
	font-weight: bold;
	text-align: center;
	/* border: 1px solid black; */
}