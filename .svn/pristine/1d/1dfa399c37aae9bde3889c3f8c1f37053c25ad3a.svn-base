@charset "UTF-8";
*{
	margin: 0;
	padding: 0;
}
#title_area{
	width: 1300px;
	height: 150px;
	margin: 0 auto;
	line-height: 60px;
	/* border: 1px solid black; */
}
#title_area h1{
	margin-top: 60px;
	text-align: center;
}
#title_area p{
	text-align: center;
}
#contents_area{
	width: 555px;
	height: 250px;
	margin: 0 auto;
	/* border: 1px solid red; */
}
#contents_area h1{
	text-align: center;
}
input{
	width: 500px;
	height: 40px;
	margin: 20px;
	border-radius: 5px;
	font-size: 15px;
	font-weight: 800;
}
input[type="button"]{
	background-color: #5EC75E;
	border-color: #5EC75E;
}
input:focus::-webkit-input-placeholder {
	color:transparent; 
}
#join_info{
	width: 555px;
	height: 50px;
	margin: 0 auto;
	padding-top: 20px;
	text-align: center;
	/* border: 1px solid blue; */
}